#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int numero;
	printf("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	scanf("%d",&numero);
	printf(" el numero es %d",numero);




	return EXIT_SUCCESS;
}
