#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int numero;
	int suma;
	printf("ingrese numero"); /* prints !!!Hello World!!! */
	scanf("%d",&numero);
	suma =numero + numero;
	printf(" el numero es %d",numero);




	return EXIT_SUCCESS;
}
