#include <stdio.h>
#include <stdlib.h>

int main(void) {

	setbuf(stdout,NULL);
	int numeroUno=0;
	int suma=0;
	int i;


	for (i=0;i<5;i++)
	{
		printf("ingrese el numero:\n");
		scanf("%d",&numeroUno);
		suma=suma+numeroUno;

	}
	printf(" La suma del numero es %d",suma);

	return EXIT_SUCCESS;
}
